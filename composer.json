{
    "name": "mstilkerich/davshell",
    "description": "Simple Shell to interact with (Card)DAV servers",
    "type": "project",
    "keywords": ["shell","carddav","contacts","owncloud","davical","nextcloud"],
    "homepage": "https://github.com/mstilkerich/davshell",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Michael Stilkerich",
            "email": "ms@mike2k.de",
            "homepage": "https://github.com/mstilkerich",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "ms@mike2k.de",
        "issues": "https://github.com/mstilkerich/davshell/issues",
        "source": "https://github.com/mstilkerich/davshell",
        "docs": "https://github.com/mstilkerich/davshell"
    },
    "require": {
        "php": ">=7.1.0",
        "mstilkerich/carddavclient": "dev-master",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "bramus/monolog-colored-line-formatter": "~3",
        "monolog/monolog": "^3",
        "sabre/vobject": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "MStilkerich\\CardDavClient\\Shell\\": "src/"
        }
    },
    "require-dev": {
        "vimeo/psalm": "~5.12.0",
        "phpcompatibility/php-compatibility": "*",
        "dealerdirect/phpcodesniffer-composer-installer": ">= 0.7.0"
    },
    "bin": ["src/davshell.php"],
    "prefer-stable" : true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
